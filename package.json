{
  "name": "node-shipping-dhl",
  "version": "1.1.0",
  "description": "DHL API for Node.JS",
  "main": "./dist/index.js",
  "scripts": {
    "build": "webpack --progress --colors",
    "coverage": "nyc npm t",
    "jshint": "jshint ./",
    "linters": "npm run jshint && npm run tslint",
    "start": "nodemon --exec ./node_modules/.bin/ts-node -- ./index.ts",
    "test": "mocha -r ts-node/register test/*.ts",
    "tslint": "tslint ./lib/"
  },
  "license": "ISC",
  "author": "Vincent Tellier <vincent@great-seal.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtllr/node-shipping-dhl"
  },
  "dependencies": {
    "crypto": "1.0.1",
    "lodash": "^4.17.4",
    "moment": "2.19.1",
    "path": "0.12.7",
    "request": "2.83.0",
    "winston": "2.4.0",
    "xml2js": "^0.4.19",
    "xmlbuilder": "9.0.4"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.5",
    "@types/chai": "4.0.4",
    "@types/chai-xml": "0.3.1",
    "@types/dotenv": "4.0.1",
    "@types/express": "4.0.37",
    "@types/mocha": "2.2.43",
    "@types/winston": "2.3.6",
    "awesome-typescript-loader": "3.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "body-parser": "1.18.2",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "chai-things": "0.2.0",
    "chai-xml": "0.3.1",
    "clean-webpack-plugin": "0.1.17",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "istanbul": "0.4.5",
    "jshint": "2.9.5",
    "mocha": "4.0.1",
    "nyc": "11.2.1",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typescript": "2.5.3",
    "webpack": "3.8.1"
  },
  "nyc": {
    "include": [
      "lib/*.ts"
    ],
    "exclude": [
      "node_modules"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
